<Window x:Class="GeoGravityOverDose.Views.Pages.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:AreaWidget="clr-namespace:GeoGravityOverDose.Views.Widget.AreaWidget"
        xmlns:CustomerWidget="clr-namespace:GeoGravityOverDose.Views.Widget.CustomerWidget"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:GeoGravityOverDose.Views.Pages"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:rxui="http://reactiveui.net"
        xmlns:ui="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:vm="clr-namespace:GeoGravityOverDose.ViewModels"
        Title="MainWindow"
        Width="800"
        Height="450"
        Style="{StaticResource MaterialDesignWindow}"
        mc:Ignorable="d">
    <Grid>
        <rxui:RoutedViewHost
            x:Name="RoutedViewHost"
            HorizontalContentAlignment="Stretch"
            VerticalContentAlignment="Stretch" />
        <ui:Snackbar x:Name="GlobalSnackbar" />
    </Grid>
</Window>
